library(tidyverse)
data = read.table('phongeo2020.txt')
View(data)
#GeoSyn
ggplot(data=data, aes(V3, V4)) +
geom_text(aes(label=V1)) +
xlab('Geo') +
ylab('Syn') +
ggtitle('Syn vs Geo. Spearman:0.487, Kendall:0.341')
cor.test(data$V3, data$V4)
cor.test(data$V3, data$V4, method='spearman')
cor.test(data$V3, data$V4, method='kendall')
